#!/bin/bash -l
#SBATCH -A uppmax2021-2-3
#SBATCH -M snowy
#SBATCH -p core
#SBATCH -n 8
#SBATCH -t 06:00:00
#SBATCH -J fly_ngs

cat $0
echo JOB = $SLURM_JOBID

# Load modules
module load bioinfo-tools bwa samtools bcftools

# Copying data to SNIC_TMP folder
cd $SNIC_TMP
cp -r /proj/uppmax2021-2-3/ngs/fly/* .
# Tried to copy the ref files there but kept getting errors.

# Align to ref
for file in f*.fq.gz
do
bwa mem -t 8 /sw/data/igenomes/Drosophila_melanogaster/Ensembl/BDGP6/Sequence/BWAIndex/genome.fa $file > "${file}".aln.sam
done

# Sort and index - normally I would pipe this and not have these two stupid loops but it's late and it's not working.
for al_file in *.sam
do
samtools sort $al_file -o "${al_file}".bam
done
for sort_file in *.bam
do
samtools index $sort_file "${sort_file}".bai
done

# Compare to ref and modify output to plain text
for bam_file in *.bam
do
bcftools mpileup -f /sw/data/igenomes/Drosophila_melanogaster/Ensembl/BDGP6/Sequence/WholeGenomeFasta/genome.fa \
$bam_file | bcftools call -mv -Ob -o "${bam_file}".bcf
done
for bcf_file in *.bcf
do
bcftools view $bcf_file -o "${bcf_file}".vcf
done

cp *.vcf ~/bigdata/ngs_lab/fly_ngs
JOB = 4497237
